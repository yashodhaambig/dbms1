create table PUBLISHER(name varchar(20), address varchar(20), phone integer,primary key(name));
create table BOOK( book_id integer, title varchar(20),  publisher_name varchar(20),  pub_year integer, primary key (book_id),  foreign key(publisher_name)  references PUBLISHER(name));
create table BOOK_AUTHORS(book_id integer,author_name varchar(20), primary key(book_id), foreign key(book_id) references BOOK(book_id));
create table   LIBRARY_BRANCH( branch_id integer,branch_name varchar(20), address varchar(20), primary key(  branch_id));  
create table BOOK_COPIES( book_id integer,branch_id integer,no_of_copies integer,primary key( book_id,branch_id), foreign key( book_id)  references BOOK (book_id))  ;  
create table BOOK_LENDING(book_id integer,branch_id integer,card_no integer,due_date integer,date_out integer,primary key(book_id,branch_id,card_no),foreign key(book_id) references BOOK(book_id),foreign key(branch_id) references LIBRARY_BRANCH(branch_id));
                               
